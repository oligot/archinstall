[user]
    name = Olivier Ligot
    email = oligot@gmail.com
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    di = diff
    dic = diff --cached -C
    unstage = reset HEAD --
    last = log -1 HEAD
[color]
    ui = auto
[core]
    autocrlf = input
    whitespace = cr-at-eol
